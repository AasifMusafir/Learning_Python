    "def asc(a,n):\n",
    "    a.sort()\n",
    "    \n",
    "    i=0\n",
    "    while i<n//2:\n",
    "        print(a[i],end=\" \")\n",
    "        i+=1\n",
    "    j=n-1 \n",
    "    while j>=n//2:\n",
    "        print(a[j],end=\" \")\n",
    "        j-=1\n",
    "        \n",
    "a=[1,45,222,23]\n",
    "asc(a,len(a))        "
   ]
